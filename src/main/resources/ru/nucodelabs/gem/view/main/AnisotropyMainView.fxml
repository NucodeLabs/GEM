<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.stage.Stage?>
<?import javafx.scene.Scene?>
<?import javafx.scene.layout.HBox?>
<?import ru.nucodelabs.gem.view.control.chart.ImageLineChart?>
<?import javafx.scene.chart.NumberAxis?>
<Stage xmlns="http://javafx.com/javafx"
       xmlns:fx="http://javafx.com/fxml"
       fx:controller="ru.nucodelabs.gem.view.main.AnisotropyMainViewController"
       title="Тестовые данные - GEM#Anisotropy">
    <scene>
        <Scene stylesheets="@../common.css">
            <VBox fx:id="root">
                <HBox>
                    <ImageLineChart fx:id="satChart">
                        <xAxis>
                            <NumberAxis/>
                        </xAxis>
                        <yAxis>
                            <NumberAxis/>
                        </yAxis>
                    </ImageLineChart>
                </HBox>
            </VBox>
        </Scene>
    </scene>
</Stage>
